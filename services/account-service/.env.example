# Telegram MTProto Configuration
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash
TELEGRAM_SESSION_DIR=./sessions

# Account credentials (можно хранить в БД или файле)
# Format: phone1:session_string1,phone2:session_string2
TELEGRAM_ACCOUNTS=

# Kafka Configuration
KAFKA_BROKERS=localhost:9093
KAFKA_GROUP_ID=account-service-group

# Logging
LOG_LEVEL=info

# Service Configuration
SERVICE_NAME=account-service
SERVICE_PORT=8084

# News Collection Settings
NEWS_POLL_INTERVAL=30s
